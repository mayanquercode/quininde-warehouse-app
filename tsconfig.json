{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@core/*": [
        "./core/*"
      ],
      "@feacture/*": [
        "./feacture/*"
      ],
      "@ui/*": [
        "./ui/*"
      ],
      "@layout/*": [
        "./layout/*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
